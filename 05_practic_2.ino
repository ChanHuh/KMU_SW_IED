int red = 12;  
void setup() {  
  // put your setup code here, to run once:  
  pinMode(red, OUTPUT);
  digitalWrite(red, HIGH);  
  delay(1000);
  digitalWrite(red, LOW);
  delay(100);
  digitalWrite(red, HIGH);
  delay(100);

  digitalWrite(red, LOW);
  delay(100);
  digitalWrite(red, HIGH);
  delay(100);

  digitalWrite(red, LOW);
  delay(100);
  digitalWrite(red, HIGH);
  delay(100);

  digitalWrite(red, LOW);
  delay(100);
  digitalWrite(red, HIGH);
  delay(100);

  digitalWrite(red, LOW);
  delay(100);
  digitalWrite(red, HIGH);
  delay(100);
  digitalWrite(red, LOW);
}
     
 
   
